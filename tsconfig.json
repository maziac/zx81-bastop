{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2022",
        "noImplicitAny": false,
        "removeComments": false,
        "noUnusedLocals": false,
        "noImplicitThis": true,
        "inlineSourceMap": false,
        "sourceMap": true,
        "outDir": "./out",
        "preserveConstEnums": true,
        "strictNullChecks": true,
        "noUnusedParameters": false,
        "allowJs": true
    },
    "exclude": [
        "examples",
        "out"
    ]
}